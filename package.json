{
  "name": "web-scraper-mongodb",
  "version": "1.0.0",
  "description": "Production-level web scraper with MongoDB storage",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node --watch src/index.ts",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "puppeteer": "^21.5.2",
    "cheerio": "^1.0.0-rc.12",
    "mongoose": "^8.0.3",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "axios": "^1.6.2",
    "user-agents": "^1.0.1147",
    "p-queue": "^7.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.9.4",
    "@types/cheerio": "^0.22.35",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "eslint": "^8.54.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "prettier": "^3.1.0",
    "@types/user-agents": "^1.0.4"
  }
}